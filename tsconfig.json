{
  "compilerOptions": {
    "outDir": "./.tsbuild/",
    "sourceMap": false,
    "target": "ES2019",
    "module": "commonjs",
    "removeComments": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "types": ["node", "@wdio/sync", "@wdio/mocha-framework"],
    "resolveJsonModule": true
  },
  "include": [
    "./bot/*.ts"
  ]
}